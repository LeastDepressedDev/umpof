{
    "id": "defpack",
    "name": "Native pack",
    "author": "qigan",
    "version": "1.0",
    "nodes": [
        {
            "id": "int_emmiter",
            "name": "Integer emmiter",
            "group": "emits",
            "width": 140,
            "description": "Emits integer value",
            "exec-type": "python",
            "exec": "emits/emit_int.py",
            "format": [
                {
                    "block": "output",
                    "text": "Integer",
                    "unid": "out",
                    "type": "int"
                }
            ],
            "prefs": [
                {
                    "id": "ival",
                    "name": "Int value",
                    "type": "int",
                    "val": "0"
                }
            ]
        },
        {
            "id": "float_emmiter",
            "name": "Float emmiter",
