        {
            "id": "int_op",
            "name": "Integer arithmetics",
            "group": "bmath",
            "width": 140,
            "description": "Provides basic arithmetical operations on integers",
            "exec-type": "python",
            "exec": "ops/ints_op.py",
            "format": [
                {
                    "block": "input",
                    "text": "Int 1",
                    "unid": "a",
                    "type": "int"
                },
               {
                    "block": "input",
                    "text": "Int 2",
                    "unid": "b",
                    "type": "int"
                },
                {
                    "block": "output",
                    "text": "Result",
                    "unid": "r",
                    "type": "int"
                }
            ],
            "prefs": [
                {
                    "id": "opcode",
                    "name": "Oparation",
                    "type": "text",
                    "val": "+"
                }
            ]
        },








        {
            "flag": "target",
            "id": "dummy_target",
            "name": "Dummy target",
            "group": "targets",
            "width": 100,
            "description": "Target that has no action",
            "exec-type": "dummy",
            "format": [
                {
                    "block": "input",
                    "text": "Any",
                    "unid": "in",
                    "type": "*"
                }
            ]
        },

        {
            "id": "tbuf",
            "name": "Text buffer",
            "group": "bufs",
            "width": 140,
            "description": "Buferize text for force casts",
            "exec-type": "python",
            "exec": "bufs/tbuf.py",
            "format": [
                {
                    "block": "input",
                    "text": "-",
                    "unid": "in",
                    "type": "text"
                },
                {
                    "block": "output",
                    "text": "+",
                    "unid": "out",
                    "type": "text"
                }
            ]
        },
        {
            "id": "sbuf",
            "name": "String buffer",
            "group": "bufs",
            "width": 140,
            "description": "Buferize text for force casts",
            "exec-type": "python",
            "exec": "bufs/sbuf.py",
            "format": [
                {
                    "block": "input",
                    "text": "-",
                    "unid": "in",
                    "type": "string"
                },
                {
                    "block": "output",
                    "text": "+",
                    "unid": "out",
                    "type": "string"
                }
            ]
        }
    ],
    "casts": [
        {
            "from": "int", 
            "to": "text", 
            "conv": "casts/itt.py" 
        },
        {
            "from": "text", 
            "to": "int", 
            "conv": "casts/tti.py" 
        },
        {
            "from": "bytes",
